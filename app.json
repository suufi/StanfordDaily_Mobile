{
  "expo": {
    "name": "Stanford Daily",
    "description": "The Stanford Daily Mobile App",
    "slug": "StanfordDaily",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "version": "1.2.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/TheStanfordDaily/StanfordDaily_Mobile",
    "orientation": "portrait",
    "primaryColor": "#94171C",
    "icon": "./assets/icon.png",
    "scheme": "stanforddaily",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#94171C"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#780A11"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.Stanford.Daily.App",
      "buildNumber": "8",
      "appStoreUrl": "https://itunes.apple.com/app/stanford-daily/id1341270063",
      "supportsTablet": true,
      "publishBundlePath": "ios/stanford-daily/Supporting/shell-app.bundle",
      "publishManifestPath": "ios/stanford-daily/Supporting/shell-app-manifest.json"
    },
    "android": {
      "package": "com.Stanford.Daily.App",
      "googleServicesFile": "./google-services.json",
      "versionCode": 10,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.Stanford.Daily.App",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "NOTIFICATIONS"
      ],
      "publishBundlePath": "android/app/src/main/assets/shell-app.bundle",
      "publishManifestPath": "android/app/src/main/assets/shell-app-manifest.json"
    },
    "notification": {
      "icon": "./assets/icon.png"
    },
    "isDetached": true,
    "detach": {
      "iosExpoViewUrl": "https://s3.amazonaws.com/exp-exponent-view-code/ios-v2.10.6-sdk32.0.0-e50ee83a-41bd-4965-b067-4c815a3b3fcc.tar.gz",
      "androidExpoViewUrl": "https://s3.amazonaws.com/exp-exponent-view-code/android-v2.10.8-sdk32.0.0-bca0f957-de78-44c8-91ed-f3151372d79d.tar.gz"
    }
  }
}